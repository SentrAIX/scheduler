# Base URL of the SentrAIX API (no trailing slash)
API_BASE_URL=https://api.sentraix.example.com
# Keycloak (preferred) - client-credentials for machine authentication
KEYCLOAK_ISSUER_URL=http://localhost:8080
KEYCLOAK_REALM=sentraix
KEYCLOAK_CLIENT_ID=sentraix-scheduler
KEYCLOAK_CLIENT_SECRET=
# Poll interval in seconds
POLL_INTERVAL_SECONDS=120
# Batch size to request per poll
BATCH_SIZE=100
# Optional: extra Axios timeout ms
HTTP_TIMEOUT_MS=30000
